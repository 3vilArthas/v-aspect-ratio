{
  "name": "v-aspect-ratio",
  "version": "1.1.0",
  "description": "Aspect ratio directive for Vue.js",
  "keywords": [
    "aspect-ratio",
    "vue"
  ],
  "homepage": "https://github.com/3vilArthas/v-aspect-ratio#readme",
  "bugs": {
    "url": "https://github.com/3vilArthas/v-aspect-ratio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3vilArthas/v-aspect-ratio.git"
  },
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Andrew Vasilchuk <andrew.d.vasilchuk@gmail.com>",
  "files": [
    "src",
    "dist",
    "vetur"
  ],
  "main": "dist/v-aspect-ratio.js",
  "unpkg": "dist/v-aspect-ratio.min.js",
  "module": "dist/v-aspect-ratio.esm.js",
  "scripts": {
    "build": "rimraf dist/* && rollup -c build/rollup.config.prod.js && npm run build:declaration",
    "build:declaration": "tsc './src/index.ts' --outDir './dist' --declaration --emitDeclarationOnly",
    "dev": "rollup -c build/rollup.config.dev.js --watch",
    "fmt": "npm run fmt:prettier",
    "fmt:prettier": "prettier --write './**/*.{js,ts,css}'",
    "lint": "npm run lint:editorconfig",
    "lint:editorconfig": "editorconfig-checker",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jest": "^26.0.14",
    "@types/randomcolor": "^0.5.5",
    "@vue/test-utils": "^1.1.0",
    "editorconfig-checker": "^3.2.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "lodash.flatten": "^4.4.0",
    "prettier": "^2.1.2",
    "randomcolor": "^0.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "vetur": {
    "attributes": "vetur/vetur-attributes.json"
  }
}
